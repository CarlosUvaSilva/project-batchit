<!-- @things & @city -->
<%= render "shared/progress-bar", type: @things.first.thing_type %>
<div class="search-bar-new">
  <div class="row">
    <div class="col-xs-12">
      <h3 class="text-center"><%= @things.first.thing_type.pluralize.capitalize %> proposition</h3>
    </div>
    <% @things.each do |thing|  %>
      <div class="col-xs-6">
        <%= render "shared/search_card", thing: thing %>
      </div>
    <% end %>
  </div>
</div>

<div class="selection_container">
  <%= render "cities/selection_container", city: @city %>
</div>

